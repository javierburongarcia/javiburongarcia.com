@import "compass/reset";

$frame_width: 15px;
$borde_stroke: 1px;
$una_col_width: 192px;
$dos_col_width: 384px;
$una_col_width_menos_frame: $una_col_width - ($frame_width * 2) - ($borde_stroke * 2);
$dos_col_width_menos_frame: $dos_col_width - ($frame_width * 2) - ($borde_stroke * 2);
$miniatura_short: 94px;
$miniatura_tall: 190px;
$fullheight: 300px;
$fullheight_menos_frame: $fullheight - ($frame_width * 2) - ($borde_stroke * 2);
$padding_overlay: 5px;

$borde: $borde_stroke dotted silver;
$borde2: $borde_stroke solid silver;
$fondo_overlay: rgba(255,255,255,0.95);

@mixin header-font {
	display: block;
	color: white;
	font-size: 20px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
}
@mixin header-tweet {
	color: white;
	font-size: 14px;
	font-family: DroidSans,Helvetica,Arial,sans-serif;
}
@mixin header-button {
	display: block;
	color: white;
	font-size: 20px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
}
@mixin header1-font {
	display: block;
	color: black;
	font-size: 20px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
	padding-bottom: 10px;
}
@mixin header2-font {
	display: block;
	color: black;
	font-size: 14px;
	line-height: 17px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
	padding-bottom: 5px;
}
@mixin body-font {
	display: block;
	color: #888;
	font-size: 12px;
	font-family: DroidSans,Helvetica,Arial,sans-serif;
}
@mixin subtitle-post-font {
	display: block;
	color: #888;
	font-size: 15px;
	font-family: DroidSans,Helvetica,Arial,sans-serif;
	padding-bottom: 40px;
}
@mixin body-post-font {
	display: block;
	color: black;
	font-size: 14px;
	line-height: 19px;
	font-family: Helvetica,Arial,sans-serif;
	padding-bottom: 20px;
}
@mixin small-font {
	display: block;
	color: #888;
	font-size: 10px;
	font-family: DroidSans,Helvetica,Arial,sans-serif;
}
a {
	color: black;
	text-decoration: none;
	&:hover .titulo{
		color: navy;
		text-decoration:underline;
	};
}
body{
	font: normal 14px/20px Helvetica,Arial
}
//Cabecera estilo
header{
	width: 100%;
	height: 60px;
	position: fixed;
	background-color: black;
	z-index: 20;
	#cabecera{
		width: 192px * 6;
		color:white;
		margin-right: auto;
		margin-left: auto;
		#botones{
			display: table;
			h1{
				height: 60px;
				width: 200px;
				display: table-cell;
				vertical-align: middle;
				z-index:  40;
				a{
					@include header-font;
				}
			}
			section.tweet{
				background: url(../images/dialog_mark.png) no-repeat 0 0;
				padding-left: 35px;
				padding-right: 35px;
				border-right: solid 1px white;
				height: 60px;
				width: 65%;
				display: table-cell;
				vertical-align: middle;
				p{
					@include header-tweet;
				}
			}
			section.socialButtons{
				height: 60px;
				width: 18%;
				display: table-cell;
				vertical-align: middle;
				.facebook a{
					background: url(../images/FacebookIcon.png) no-repeat;
					background-position:center 12px;
				}
				.twitter a{
					background: url(../images/TwitterIcon.png) no-repeat;
					background-position:center 12px;
				}
				a{
					box-sizing: border-box;
					float:right;
					width:70px;
					padding-top: 35px;
					padding-left: 10px;
					padding-right: 10px;
					display: table-cell;
					height: 60px;
					text-align: center;
					vertical-align: middle;
					color: rgba(255,255,255,0.75);
					font-size: 11px;
					font-family: DroidSans,Helvetica,Arial,sans-serif;
					
					&:hover{
						color: rgba(255,255,255,1);
						background-position:center -36px;
					};
				}
			}
		}
	}
}
.cuerpo{
	width: 192px * 6;
	margin-right: auto;
	margin-left: auto;
	padding-top: 100px;
}
footer{
	padding-top: 40px;
	padding-bottom: 40px;
	clear: both;
	@include small-font;
	text-align: center;
}
.cuerpo ul{
	width: 192px * 6;
}
.cuerpo li{
	height: $fullheight;
	float: left;
	position: relative;
	&.una-col:last-child a{
		width: $una_col_width + $borde_stroke;
		border-right: $borde;
	}
	&.dos-col:last-child a{
		width: $dos_col_width + $borde_stroke;
		border-right: $borde;
	}
	 a{
		box-sizing: border-box;
		position: relative;
		overflow: hidden;
		float: left;
		padding: $frame_width;
		height: $fullheight;
		border-left: $borde;
		border-bottom: $borde;
		&.cierrefila{
			border-right: $borde;
		}
		&.fila1{
			border-top: $borde;
		}
		.miniatura{
			margin: 0px;
			overflow: hidden;
			display: block;
			text-align: center;
			vertical-align: middle;
			img {
			  display: inline;
			}
		}
		.texto{
			box-sizing: border-box;
			.subtitulo{
				@include body-font;
				display: block;
			}
		}
		.fecha{
			box-sizing: border-box;
			@include small-font;
			text-align: right;
			position: absolute;
			right: $frame_width;
			bottom: 6px;
		}
	}
}
/*Estilos de fondo imagen*/
.image-fullscreen {
	position:fixed;
	left:0px;
	top:0px;
	right:0px;
	bottom:0px;
	height:1000px;
	z-index: 0;
}
.cuerpoPost {
	padding-top: 100px;
	width: 192px * 6;
	margin: 0px auto;
 	section{
		width: 450px;
		margin: 0px auto 0px 0px;
		position: relative;
		z-index: 100;
	}
	.tituloPost{
		@include header1-font;
	}
	.subtituloPost{
		@include subtitle-post-font;
	}
	.bodyPost p{
		@include body-post-font;
	}
}

/*Estilos de anchos de columna*/

@mixin una-col{
	width: $una_col_width;
	a{
		width: $una_col_width;
	}
}
@mixin dos-col{
	width: $dos_col_width;
	a{
		width: $dos_col_width;
	}
}


/*Estilos de ocultar*/

@mixin oculta-subtitulo{
	a{
		.texto{
			.subtitulo{
				display: none;
			}
		}
	}
}
@mixin oculta-fecha{
	a{
		.fecha{
			display: none;
		}
	}
}

/*Estilos de imagenes*/
//Aqui se definen todos los estilos de imagenes disponibles

@mixin imagen-horizontal-small{
	.miniatura{
		margin-bottom: 10px;
		width: $una_col_width_menos_frame;
		height: $miniatura_short;
		img{
			width: $una_col_width_menos_frame;
		}
	}
}
@mixin imagen-horizontal{
	.miniatura{
		margin-bottom: 10px;
		width: $dos_col_width_menos_frame;
		height: $miniatura_tall;
		img{
			width: $dos_col_width_menos_frame;
		}
	}
}
@mixin imagen-horizontal-full{
	.miniatura{
		margin-bottom: 10px;
		width: $dos_col_width_menos_frame;
		height: $fullheight_menos_frame + ($borde_stroke * 2);
		img{
			height: $fullheight_menos_frame + ($borde_stroke * 2);
		}
	}
}
@mixin imagen-vertical{
	.miniatura{
		float:left;
		margin-bottom: 10px;
		width: $una_col_width_menos_frame;
		height: $miniatura_tall;
		img{
			height: $miniatura_tall;
		}
	}
}
@mixin imagen-vertical-full-narrow{
	.miniatura{
		width: $una_col_width_menos_frame;
		height: $fullheight_menos_frame + ($borde_stroke * 2);
	}
}
@mixin imagen-vertical-full{
	.miniatura{
		margin-right: $frame_width;
		width: $una_col_width - $frame_width;
		height: $fullheight_menos_frame + ($borde_stroke * 2);
		img{
			height: $fullheight_menos_frame + ($borde_stroke * 2);
		}
	}
}


/*Estilos de texto*/
//Aqui se definen todos los estilos de texto disponibles

@mixin texto-una-col{
	.texto{
		.titulo{
			@include header2-font;
		}
	}
}
@mixin texto-una-col-sinfoto{
	.texto{
		.titulo{
			@include header1-font;
		}
	}
}
@mixin texto-dos-col{
	.texto{
		position: absolute;
		top:$frame_width;
		left: $una_col_width + $frame_width;
		.titulo{
			@include header1-font;
		}
	}
}
@mixin texto-caption-una-col{
	.texto{
		.titulo{
			@include header2-font;
		}
		.subtitulo{
			text-overflow: ellipsis;
			overflow:hidden; 
			line-height: 1.2em; 
			max-height: 1.2em;
		}
	}
}
@mixin texto-caption-dos-col{
	.texto{
		.titulo{
			@include header2-font;
		}
		.subtitulo{
			text-overflow: ellipsis;
			overflow:hidden; 
			line-height: 1.2em; 
			max-height: 1.2em;
		}
	}
}
@mixin texto-overlay-una-col{
	.texto{
		text-align: right;
		background: $fondo_overlay;
		position: absolute;
		width: $una_col_width_menos_frame;
		height: $fullheight - $miniatura_tall - 2*$frame_width;
		left: $frame_width;
		border: $borde2;
		bottom: $frame_width - 2*$borde_stroke;
		padding: $padding_overlay;
		.titulo{
			@include header2-font;
		}
		.subtitulo{
			text-overflow: ellipsis;
			overflow:hidden; 
			line-height: 1.2em; 
			max-height: 1.2em;
		}
	}
}
@mixin texto-overlay-dos-col{
	@include texto-overlay-una-col;
	.texto{
		width: $dos_col_width_menos_frame;
	}
}


/*Layouts*/
//Aqui se definen los layouts para todos los tipos de post, intentar que todo este hecho utilizando includes

//Layout la1
li.texto.una-col{
	@include una-col;
	@include texto-una-col-sinfoto;
}
//Layout la2
li.video.una-col,
li.articulo.una-col, li.proyecto.una-col{	
	@include una-col;
	@include imagen-horizontal-small;
	@include texto-una-col;
}
//Layout la3
li.articulo.dos-col,
li.proyecto.dos-col,
li.link.dos-col,
li.pelicula.dos-col, li.libro.dos-col, li.documento.dos-col{
	@include dos-col;
	@include imagen-vertical-full;
	@include texto-dos-col;
}
//Layout la4A
li.link.una-col{
	@include una-col;
	@include imagen-vertical;
	@include texto-caption-una-col;
}
//Layout la4B
li.foto.dos-col,
li.presentacion.dos-col{
	@include dos-col;
	@include imagen-horizontal-full;
	@include texto-overlay-dos-col;
	@include oculta-fecha;
}
//Layout la4C
li.video.dos-col{
	@include dos-col;
	@include imagen-horizontal;
	@include texto-caption-dos-col;
}
//Layout la5A
li.pelicula.una-col, li.libro.una-col, li.documento.una-col{
	@include una-col;
	@include imagen-vertical;
	@include texto-caption-una-col;
}
//Layout la5B
li.presentacion.una-col,
li.foto.una-col{
	@include una-col;
	@include imagen-vertical-full-narrow;
	@include texto-overlay-una-col;
	@include oculta-subtitulo;
	@include oculta-fecha;
