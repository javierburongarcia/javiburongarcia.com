footer{
	padding-top: 40px;
	padding-bottom: 40px;
	clear: both;
	@include small-font;
	text-align: center;
}
// Estilos en vista principal
#cuerpo {
	width: 192px * 6;
	margin-right: auto;
	margin-left: auto;
	padding-top: 100px;
	ul{
		width: 192px * 6;
		li{
			height: $fullheight;
			float: left;
			position: relative;
			&.una-col:last-child a{
				width: $una_col_width + $borde_stroke;
				border-right: $borde;
			}
			&.dos-col:last-child a{
				width: $dos_col_width + $borde_stroke;
				border-right: $borde;
			}
			a{
				box-sizing: border-box;
				position: relative;
				overflow: hidden;
				float: left;
				padding: $frame_width;
				height: $fullheight;
				border-left: $borde;
				border-bottom: $borde;
				&.cierrefila{
					border-right: $borde;
				}
				&.fila1{
					border-top: $borde;
				}
				.miniatura{
					box-sizing: border-box;
					overflow: hidden;
				}
				.texto{
					box-sizing: border-box;
					.subtitulo{
						@include body-font;
					}
				}
				.fecha{
					box-sizing: border-box;
					@include small-font;
					text-align: right;
					position: absolute;
					right: $frame_width + 15px;
					bottom: 6px;
				}
				.icono{
					position: absolute;
					width: 20px;
					height: 20px;
					right: 8px;
					bottom: 8px;
					background: url(../images/iconos.png) no-repeat;
					&.proyecto{
						background-position: 0 0px;
					}
					&.libro{
						background-position: 0 -20px;
					}
					&.foto{
						background-position: 0 -40px;
					}
					&.video{
						background-position: 0 -60px;
					}
					&.texto, &.articulo {
						background-position: 0 -80px;
					}
					&.pelicula{
						background-position: 0 -100px;
					}
					&.link{
						background-position: 0 -120px;
					}
				}
			}
		}
	}
}
/*Estilos de vistas individuales*/
.colorbackground-fullscreen {
	position:fixed;
	left:0px;
	top:0px;
	right:0px;
	bottom:0px;
	height:1000px;
	z-index: -300;
}
.image-fullscreen {
	background: 50% 50% no-repeat;
	position:fixed;
	left:0px;
	top:0px;
	right:0px;
	bottom:0px;
	height:1000px;
	z-index: -200;
}

