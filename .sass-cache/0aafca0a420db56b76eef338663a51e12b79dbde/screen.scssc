@import "compass/reset";

$frame_width_fino: 8px;
$frame_width: 15px;
$borde_stroke: 1px;
$una_col_width: 192px;
$dos_col_width: 384px;
$una_col_width_menos_frame: $una_col_width - ($frame_width * 2) - ($borde_stroke * 2);
$dos_col_width_menos_frame: $dos_col_width - ($frame_width * 2) - ($borde_stroke * 2);
$miniatura_short: 94px;
$miniatura_tall: 190px;
$fullheight: 300px;
$fullheight_menos_frame: $fullheight - ($frame_width * 2) - ($borde_stroke * 2);
$padding_overlay: 5px;
$borde: $borde_stroke solid rgb(220,220,220);
$borde2: $borde_stroke solid silver;
$fondo_overlay: rgba(255,255,255,0.95);
$fondo_overlay2: rgba(255,255,255,0.90);


@mixin header-font {
	display: block;
	color: white;
	font-size: 20px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
}
@mixin header-tweet {
	color: white;
	font-size: 14px;
	font-family: DroidSans,Helvetica,Arial,sans-serif;
}
@mixin header-button {
	display: block;
	color: white;
	font-size: 20px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
}
@mixin header1-font {
	display: block;
	color: black;
	font-size: 20px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
	padding-bottom: 10px;
}
@mixin header2-font {
	display: block;
	color: black;
	font-size: 14px;
	line-height: 17px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
	padding-bottom: 5px;
}
@mixin body-font {
	display: block;
	color: #888;
	font-size: 12px;
	font-family: DroidSans,Helvetica,Arial,sans-serif;
}
@mixin small-font {
	display: block;
	color: #888;
	font-size: 10px;
	font-family: DroidSans,Helvetica,Arial,sans-serif;
}
@mixin title-post-font {
	display: block;
	font-size: 20px;
	font-family: DroidSerif,Helvetica,Arial,sans-serif;
	padding-bottom: 30px;
}
@mixin subtitle-post-font {
	display: block;
	opacity: 0.65;
	font-size: 16px;
	font-family: DroidSans,Helvetica,Arial,sans-serif;
	padding-bottom: 40px;
}
@mixin body-post-font {
	display: block;
	font-size: 14px;
	line-height: 20px;
	font-family: Helvetica,Arial,sans-serif;
	padding-bottom: 20px;
}
a {
	color: black;
	text-decoration: none;
	&:hover .titulo{
		color: navy;
		text-decoration:underline;
	};
}
body{
	font: normal 14px/20px Helvetica,Arial
}
//Loader estilo
#loader_background{
	display:none;
	font: normal 16px/20px Helvetica,Arial;
	color:black;
	background: rgba(230,230,230,0.5);
	position:fixed;
	width:100%;
	height:100%;
	text-align: center;
	z-index:300;
	#loader_image{
		position: absolute;
		top:50%;
		left:50%;
		background: url(../images/loader_image.gif) no-repeat;
		width:32px;
		height:32px;
		opacity: 0.75;
	}
}
//Cabecera estilo
header{
